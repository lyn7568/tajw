.exchange_dialog {
    position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 110;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-box-pack: end;
    background-color: rgba(0,0,0,0.5);
    animation: 0.2s show_bg_anim linear;
}

@-webkit-keyframes show_anim {
    0% {
        transform: translateY(100%);
    }

    100% {
        transform: translateY(0);
    }
}

@keyframes show_anim {
    0% {
        transform: translateY(100%);
    }

    100% {
        transform: translateY(0);
    }
}

@-webkit-keyframes hide_down_anim {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(100%);
    }
}

@keyframes hide_down_anim {
    0% {
        transform: translateY(0);
    }

    100% {
        transform: translateY(100%);
    }
}

@-webkit-keyframes hide_bg_anim {
    0% {
        background-color: rgba(0,0,0,0.5);
    }

    100% {
        background-color: rgba(0,0,0,0);
    }
}

@keyframes hide_bg_anim {
    0% {
        background-color: rgba(0,0,0,0.5);
    }

    100% {
        background-color: rgba(0,0,0,0);
    }
}

@-webkit-keyframes show_bg_anim {
    0% {
        background-color: rgba(0,0,0,0);
    }

    100% {
        background-color: rgba(0,0,0,0.5);
    }
}

@keyframes show_bg_anim {
    0% {
        background-color: rgba(0,0,0,0);
    }

    100% {
        background-color: rgba(0,0,0,0.5);
    }
}

.exchange_dialog_container {
