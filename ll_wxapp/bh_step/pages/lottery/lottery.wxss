.notice {
    display: flex;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    background: #ff6600;
    height: 60rpx;
    padding: 0 20rpx;
}

.barrage_box {
    display: flex;
    width: 0px;
